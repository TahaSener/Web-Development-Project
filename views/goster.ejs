<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <title>Filocum</title>

  <!-- Google Fonts -->
  <link href="https://fonts.googleapis.com/css?family=Montserrat|Ubuntu" rel="stylesheet">

  <!-- CSS Stylesheets -->
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
  <link rel="stylesheet" href="/css/styles.css">

  <!-- Font Awesome -->
  <script defer src="https://use.fontawesome.com/releases/v5.0.7/js/all.js"></script>

  <!-- Bootstrap Scripts -->
  <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
</head>

<body>
  <section class="colored-section" id="titlearac">

    <div class="container-fluid" id='containerarac'>

      <!-- Nav Bar -->

      <nav class="navbar navbar-expand-lg navbar-dark" id='navbararac'>

        <a class="navbar-brand" href="">Filocum</a>

        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarTogglerDemo02">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarTogglerDemo02">

          <ul class="navbar-nav ml-auto">
            <li class="nav-item">
              <a class="nav-link" href="/AracEkle">Arac Ekle</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/StokEkle">Stok Ekle</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/AdminPanel">Panel</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/AdminKayıt">Admin Ekle</a>
            </li>
          </ul>

        </div>
      </nav>
    </div>

    </section>



    <section class="white-section" id="pricing">

      <div class="row">
        <table class="table table-bordered col-lg-12 col-md-12 text-center" id=tablo>
            <thead>
              <tr>
                <% for (var i = 0; i < Columns.length; i++) { %>
                  <th scope="col"><%=JSON.stringify(Columns[i].COLUMN_NAME).replace(/"/g, "")%></th>
                  <% } %>
                  <th scope="col">İşlem</th>
              </tr>
            </thead>
            <tbody>
              <% for (var i = 0; i < Tablo.length; i++) { %>
              <tr name='<%=JSON.stringify(i).replace(/"/g, "")%>'>
                <% for (const[key,value] of Object.entries(Tablo[i])) { %>
                  <th  name='<%=JSON.stringify(key).replace(/"/g, "")%>' scope="row"><%=JSON.stringify(value).replace(/"/g, "")%></th>
                  <% } %>
                  <th scope="row"><button type="button" class="button" name='<%=JSON.stringify(i).replace(/"/g, "")%>'>sil</button> </th>
              </tr>
              <% } %>
            </tbody>
          </table>

      </div>

    </section>
    <!-- Call to Action -->

    <script type="text/javascript">
        $('.button').on('click',function() {
          var ioe=this.name;
          var ss=$("tr[name="+ioe+"]").children()[0];
          var id=ss.textContent;
          urlde=""+window.location.pathname+"/Sil/"+id+"";
          window.location.href=urlde;
        })

    </script>


    <!-- Footer -->



    </body>

    </html>
