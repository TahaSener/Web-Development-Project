<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <title>Filocum</title>

  <!-- Google Fonts -->
  <link href="https://fonts.googleapis.com/css?family=Montserrat|Ubuntu" rel="stylesheet">

  <!-- CSS Stylesheets -->
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
  <link rel="stylesheet" href="/css/styles.css">

  <!-- Font Awesome -->
  <script defer src="https://use.fontawesome.com/releases/v5.0.7/js/all.js"></script>

  <!-- Bootstrap Scripts -->
  <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
</head>

<body>

  <section class="colored-section" id="titlearac">

    <div class="container-fluid" id='containerarac'>

      <!-- Nav Bar -->

      <nav class="navbar navbar-expand-lg navbar-dark" id='navbararac'>

        <a class="navbar-brand" href="">Filocum</a>

        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarTogglerDemo02">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarTogglerDemo02">

          <ul class="navbar-nav ml-auto">
            <li class="nav-item">
              <a class="nav-link" href="#footer">İletişim</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="#pricing">Hakkımızda</a>
            </li>
          </ul>

        </div>
      </nav>


    </section>

    <section class="white-section" id="pricing">

      <h2 class="section-heading">Her ölçekteki firmalar için</h2>
      <p>Basit ve fiyat/performans ürünleri.</p>

      <div class="row">

          <% for (var i = 0; i < DataArac.length; i++) { %>
            <div class="pricing-column col-lg-4 col-md-6">
              <div class="card">
                <div class="card-header">
                  <h3><%=JSON.stringify(DataArac[i].Marka).replace(/"/g, "") + " " + JSON.stringify(DataArac[i].Model).replace(/"|\t| /g, "")%></h3>
                </div>
                <div class="card-body">
                  <% var aracadı=JSON.stringify(DataArac[i].Model).replace(/"|\t| /g, "") %>
                  <img src="images/<%=aracadı%>.png" class="img-fluid" id=Araclarpreview alt="Responsive image">
                  <h3 class="price-text"><%=JSON.stringify(DataArac[i].Fiyat)%> TL / günlük</h2>
                  <p><%=JSON.stringify(DataArac[i].Kalan_Adet) + " " + "Adet Stokta"%></p>
                  <p><%=JSON.stringify(DataArac[i].Yakıt_Türü)%></p>
                  <button class="btn btn-lg btn-block btn-outline-dark" onclick="location.href='/Araclar/<%=aracadı%>'" type="button">Detaylar</button>
                </div>
              </div>
            </div>
          <% } %>

      </div>

    </section>
    <!-- Call to Action -->

    <section class="colored-section" id="cta">

      <div class="container-fluid">

        <h3 class="big-heading">İstediğiniz filoyu şimdi oluşturun.</h3>
        <button class="download-button btn btn-lg btn-dark" type="button"><i class="fab fa-apple"></i> İndir</button>
        <button class="download-button btn btn-lg brn-light" type="button"><i class="fab fa-google-play"></i> İndir</button>
      </div>
    </section>


    <!-- Footer -->

    <footer class="white-section" id="footer">
      <div class="container-fluid">
        <i class="social-icon fab fa-facebook-f"></i>
        <i class="social-icon fab fa-twitter"></i>
        <i class="social-icon fab fa-instagram"></i>
        <i class="social-icon fas fa-envelope"></i>
        <p>© Copyright 2022 Filocum</p>
      </div>
    </footer>


    </body>

    </html>
